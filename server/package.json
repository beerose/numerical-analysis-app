{
  "name": "server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "watch-compile": "cross-env PORT=3003 parcel ./src/index.ts --target=node",
    "watch-run": "cross-env PORT=8082 nodemon dist/index.js",
    "build": "cross-env PORT=3003 parcel build ./src/index.ts --target=node",
    "launch": "cross-env PORT=8082 node dist/index.js",
    "start": "yarn build && node dist/index.js",
    "dev": "concurrently -c blue,yellow -n n,p \"yarn watch-run\" \"yarn watch-compile\""
  },
  "dependencies": {
    "@types/atob": "^2.1.1",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.11.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/js-cookie": "^2.2.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.5",
    "@types/nodemailer": "^4.6.5",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/uniqid": "^4.1.2",
    "atob": "^2.1.2",
    "bcrypt": "^3.0.3",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "io-ts": "^1.5.2",
    "io-ts-reporters": "^0.0.21",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.16.0",
    "nodemailer": "^5.0.0",
    "parcel-bundler": "^1.11.0",
    "swagger-ui-express": "^4.0.1",
    "uniqid": "^5.0.3"
  },
  "repository": {},
  "license": "MIT",
  "devDependencies": {
    "@types/mysql": "^2.15.5",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "morgan-body": "^2.4.5",
    "nodemon": "^1.18.9",
    "prettier": "^1.11.1",
    "supervisor": "^0.12.0",
    "typescript": "^3.2.2"
  }
}
