{
  "name": "server",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "watch-compile": "PORT=3003 parcel watch ./src/index.ts --target=node",
    "watch-run": "PORT=3000 nodemon dist/index.js",
    "build": "PORT=3003 parcel build ./src/index.ts --target=node",
    "launch": "node dist/index.js",
    "start": "yarn build && node dist/index.js",
    "dev": "concurrently -c red,yellow -n n,p 'yarn watch-run' 'yarn watch-compile'"
  },
  "dependencies": {
    "@types/atob": "^2.1.1",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/morgan": "^1.7.35",
    "@types/mysql": "^2.15.5",
    "@types/nodemailer": "^4.6.5",
    "@types/swagger-ui-express": "^3.0.0",
    "atob": "^2.1.2",
    "cors": "^2.8.4",
    "dotenv": "^6.1.0",
    "express": "^4.16.3",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.16.0",
    "nodemailer": "^4.6.8",
    "parcel-bundler": "^1.8.3",
    "swagger-ui-express": "^4.0.1"
  },
  "repository": {},
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^4.0.1",
    "morgan-body": "^2.4.5",
    "nodemon": "^1.18.6",
    "prettier": "^1.11.1",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.10.0",
    "typescript": "^2.8.3"
  }
}
